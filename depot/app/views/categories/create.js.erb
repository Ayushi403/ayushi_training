
<%if @category.errors.any? %>
	$('#error_msg').html("<div id='error_explanation'> <h2><%= pluralize(@category.errors.count, 'error') %> prohibited this category from being saved:</h2> <ul> <% @category.errors.full_messages.each do |msg| %> <li><%= msg %></li> <% end %> </ul> </div>");
<% else %>
	$('#listing').append("<%= j render partial: 'cat_list' %>")
	$('#cat_form').load(document.URL +  ' #cat_form');
<% end %>